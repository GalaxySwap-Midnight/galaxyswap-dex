pragma language_version >= 0.14.0;

/**
 * @description An interface module for Uint<128> types using U128 struct.
 */
module Uint128 {
    /**
     * @description A pure circuit that returns the modulus value of U128.
     *
     * @returns None
     */
    export pure circuit MODULUS(): Uint<128> {
        // pow() is not supported yet so we hardcode this value.
        // Uint128_MODULUS as a modulus value, 2^64
        return 18446744073709551616;
    }
}
