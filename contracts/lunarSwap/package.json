{
  "name": "@midnight-dapps/lunarswap-contracts",
  "version": "1.0.0-alpha.1",
  "description": "Compacts for implementing LunarSwap DEX",
  "type": "module",
  "scripts": {
    "compact": "find src -name '*.compact' -exec sh -c 'run-compactc \"{}\" \"src/artifacts/$(basename \"{}\" .compact)\"' \\;",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -Rf ./src/artifacts ./dist/artifacts && cp ./src/counter.compact ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "typescript": "~5.7.2"
  },
  "dependencies": {
    "@midnight-dapps/compact": "workspace:^"
  }
}
